{
  "name": "@fpkit/nextjs",
  "description": "Nextjs components",
  "private": false,
  "version": "0.2.5",
  "type": "module",
  "source": "src/index.ts",
  "main": "./libs/index.cjs",
  "module": "./libs/index.js",
  "types": "./libs/index.d.ts",
  "files": [
    "libs",
    "src",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && tsup --dts",
    "preview": "vite preview",
    "package": "tsc && tsup --dts",
    "package:watch": "tsc && tsup --watch --dts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:ci": "c8 vitest --ci --coverage",
    "test:update": "vitest -u",
    "trash": "trash 'libs'"
  },
